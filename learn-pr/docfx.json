                    "**/*.yml"
                ],
                "exclude": [
                    "**/includes/**"
                ],
                "dest": "modules"
            },
            {
                "files": [
                    "**/*.md",
                    "**/*.yml"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/includes/**",
                    "_themes/**",
                    "_themes.pdf/**",
                    "README.md",
                    "LICENSE",
                    "LICENSE-CODE",
                    "ThirdPartyNotices"
                ]
            }
        ],
        "resource": [{
                "src": "azure",
                "files": [
                    "**/*.gif",
                    "**/*.png",
                    "**/*.svg",
                    "**/*.jpg",
                    "**/*.jpeg"
                ],
                "dest": "modules"
            },
            {
                "files": [
                    "**/*.gif",
                    "**/*.png",
                    "**/*.svg",
                    "**/*.jpg",
