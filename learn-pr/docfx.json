                ],
                "exclude": [
                    "**/obj/**",
                    "**/includes/**",
                    "_themes/**",
                    "_themes.pdf/**"
                ]
            }
        ],
        "overwrite": [],
        "externalReference": [],
        "globalMetadata": {
            "show_latex": true,
            "breadcrumb_path": "/learn/breadcrumb/toc.json",
            "feedback_system": "GitHub",
            "feedback_github_repo": "MicrosoftDocs/learn",
            "clicktale": true,
            "uhfHeaderId": "MSDocsHeader-Learn",
            "localization_scopes": [
                "de-de",
                "es-es",
                "fr-fr",
                "it-it",
                "ja-jp",
                "pt-br",
                "zh-cn",
                "zh-tw",
