                ],
                "exclude": [
                    "**/includes/**"
                ],
                "dest": "modules"
            },
            {
                "src": "ai-business-school",
                "files": [
                    "**/*.yml"
                ],
                "exclude": [
                    "**/includes/**"
                ],
                "dest": "modules"
            },
            {
                "src": "azure",
                "files": [
                    "**/*.yml"
                ],
                "exclude": [
                    "**/includes/**"
                ],
                "dest": "modules"
            },
            {
                "src": "data-ai-cert",
                "files": [
                    "**/*.yml"
                ],
                "exclude": [
                    "**/includes/**"
                ],
                "dest": "modules"
            },
            {
                "src": "databricks",
                "files": [
                    "**/*.yml"
                ],
                "exclude": [
                    "**/includes/**"
                ],
                "dest": "modules"
